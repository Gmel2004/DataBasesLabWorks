<Window x:Class="ChessTactics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChessTactics"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="ENTER AS ADMIN" Command="{Binding EnterAsAdmin}"/>
        <TextBox TextWrapping="Wrap" Width="120" Text="{Binding Password}" Name="code"/>
        <Label Content="OR"/>
        <Button Content="ENTER AS READER" Command="{Binding EnterAsUser}"/>
    </StackPanel>
</Window>
